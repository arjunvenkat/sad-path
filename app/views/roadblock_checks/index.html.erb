<h1>Listing roadblock_checks</h1>

<table>
  <thead>
    <tr>
      <th>Check</th>
      <th>Roadblock</th>
      <th>Completed at</th>
      <th>Solved it</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @roadblock_checks.each do |roadblock_check| %>
      <tr>
        <td><%= roadblock_check.check_id %></td>
        <td><%= roadblock_check.roadblock_id %></td>
        <td><%= roadblock_check.completed_at %></td>
        <td><%= roadblock_check.solved_it %></td>
        <td><%= link_to 'Show', roadblock_check %></td>
        <td><%= link_to 'Edit', edit_roadblock_check_path(roadblock_check) %></td>
        <td><%= link_to 'Destroy', roadblock_check, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Roadblock check', new_roadblock_check_path %>
